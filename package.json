{
  "name": "swagen",
  "version": "0.0.27",
  "description": "Client code generator for Swagger JSON",
  "main": "cli.js",
  "preferGlobal": true,
  "scripts": {
    "bump": "npm version patch --no-git-tag-version",
    "bumppub": "npm run bump && npm publish",
    "update": "npm un --save swagen-core && npm i --save swagen-core"
  },
  "bin": {
    "swagen": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular-template/swagger-client.git"
  },
  "typings": "typings/index.d.ts",
  "author": "Angular Template",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/angular-template/swagger-client/issues"
  },
  "homepage": "https://github.com/angular-template/swagger-client#readme",
  "dependencies": {
    "inquirer": "^2.0.0",
    "lodash": "^4.17.0",
    "minimist": "^1.2.0",
    "needle": "^1.4.3",
    "swagen-core": "0.0.14"
  }
}
